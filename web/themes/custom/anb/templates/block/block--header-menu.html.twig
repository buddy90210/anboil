{% do attributes.removeAttribute('id') %}
{% spaceless %}
<span class="{{ block_class }}__trigger visually-hidden">
  <svg width="20" height="14" viewBox="0 0 20 14" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M1 7H19M1 1H19M1 13H19" stroke="var(--base-color)" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg>
</span>
<nav{{ attributes.addClass(block_class) }}>
  {{ title_prefix }}
  {{ title_suffix }}
  <span class="{{ block_class }}__close close-icon visually-hidden"></span>
  {% block content %}
    {{ content }}
  {% endblock %}
</nav>
{% endspaceless %}
